{
  "id": "plugin.com.pot-app.ocrspace",
  "plugin_type": "recognize",
  "icon": "icon.png",
  "display": "OCR Space",
  "homepage": "https://github.com/pot-app/pot-app-recognize-plugin-template",
  "needs": [
      {
        "key": "api_url",
        "display": "API URL",
        "type": "input",
        "placeholder": "https://api.example.com/v1"
      },
      {
        "key": "apikey",
        "display": "API Key",
        "type": "input"
      },
      {
        "key": "model",
        "display": "Model",
        "type": "select",
        "options": {
          "gpt-4o": "GPT-4o",
          "gpt-4o-mini": "GPT-4o-Mini",
          "gpt-4-vision-preview": "GPT-4 Vision Preview"
        },
        "dynamic": true
      },
      {
        "key": "customPrompt",
        "display": "自定义 Prompt",
        "type": "input",
        "placeholder": "从该图像中提取文本"
      }
    ],
  "language": {
    "auto": "eng",
    "zh_cn": "chs",
    "zh_tw": "cht",
    "en": "eng",
    "ja": "jpn",
    "ko": "kor",
    "fr": "fre",
    "es": "spa",
    "ru": "rus",
    "de": "ger",
    "it": "ita",
    "tr": "tur",
    "pt_pt": "por",
    "pt_br": "por",
    "vi": "vie",
    "th": "tai",
    "ar": "ara",
    "hi": "hin",
    "fa": "per"
  }
}
